          <mj-text font-size="11px" color="#888888" letter-spacing="3px" text-transform="uppercase" padding-bottom="12px">
            New Season
          </mj-text>
          <mj-text css-class="no-wrap " font-size="34px" font-weight="700" color="#0a0a0a" line-height="36px" padding-bottom="12px">
