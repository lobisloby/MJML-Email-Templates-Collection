<mjml>
  <mj-head>
    <mj-title>Fashion Newsletter - Autumn Collection</mj-title>
    <mj-preview>Upgrade Your Style - New Collection Out Now</mj-preview>

    <mj-font name="Inter" href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap" />

    <mj-attributes>
      <mj-all font-family="'Inter', 'Helvetica Neue', Helvetica, Arial, sans-serif" />
      <mj-text font-size="14px" color="#333333" line-height="16px" />
      <mj-button font-size="13px" font-weight="500" />
      <mj-section padding="0" />
      <mj-column padding="0" />
    </mj-attributes>

    <mj-style>
      .shadow-card {
        box-shadow: 0 8px 40px rgba(0, 0, 0, 0.08) !important;
        border-radius: 4px;
        overflow: hidden;
      }

      .nav-link {
        text-decoration: none !important;
        color: #333333 !important;
        font-weight: 500;
      }

      .nav-link:hover {
        color: #000000 !important;
      }

      .product-card {
        transition: transform 0.2s ease;
      }

      .cta-primary {
        background-color: #000000 !important;
        color: #ffffff !important;
      }

      .cta-secondary {
        background-color: #ffffff !important;
        color: #333333 !important;
        border: 1px solid #333333 !important;
      }

      .no-wrap div {
        white-space: nowrap;
      }

      /* Mobile styles */
      @media only screen and (max-width: 480px) {
        .product-card {
          margin-bottom: 30px !important;
        }

        /* Remove margin from last card */
        .product-card:last-child {
          margin-bottom: 0 !important;
        }
      }
    </mj-style>

    <mj-style inline="inline">
      a {
        text-decoration: none;
      }

      .star {
        color: #F4B942;
        font-size: 12px;
      }
    </mj-style>

    <mj-breakpoint width="480px" />



  </mj-head>

  <mj-body background-color="#F5F0EB">

    <!-- Spacer for page padding -->
    <!-- <mj-section padding="0px" background-color="#F5F0EB">
      <mj-column><mj-text>&nbsp;</mj-text></mj-column>
    </mj-section> -->

    <!-- ========== MAIN EMAIL CARD WRAPPER ========== -->
    <mj-wrapper background-color="#ffffff" padding="0" css-class="shadow-card" border-radius="4px">

      <!-- ========== TOP BLACK TAGLINE BAR ========== -->
      <mj-section background-color="#0a0a0a" padding="12px 25px">
        <mj-column>
          <mj-text align="center" color="#ffffff" font-size="11px" letter-spacing="2px" font-weight="400" text-transform="uppercase">
            ✦ Autumn & Clothes Collection ✦
          </mj-text>
        </mj-column>
      </mj-section>

      <!-- ========== NAVIGATION BAR ========== -->
      <mj-section background-color="#ffffff" padding="18px 25px" border-bottom="1px solid #f0f0f0">
        <!-- Hamburger Menu -->
        <!-- <mj-column width="10%" vertical-align="middle">
          <mj-image src="https://img.icons8.com/ios/100/000000/menu--v1.png" width="22px" alt="Menu" padding="0" />
        </mj-column> -->

        <!-- Nav Links -->
        <mj-column width="65%" vertical-align="middle">
          <mj-text align="left" font-size="13px" padding="0">
            <a href="#" class="nav-link" style="color: #333; margin: 0 14px; font-weight: 500;">Men</a>
            <a href="#" class="nav-link" style="color: #333; margin: 0 14px; font-weight: 500;">Women</a>
            <a href="#" class="nav-link" style="color: #333; margin: 0 14px; font-weight: 500;">New Arrivals</a>
            <a href="#" class="nav-link" style="color: #d4442e; margin: 0 14px; font-weight: 600;">Sale</a>
          </mj-text>
        </mj-column>

        <!-- Icons -->
