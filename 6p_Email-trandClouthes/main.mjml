        </mj-column>

        <!-- Product 3 -->
        <mj-column width="33.33%" padding="0 10px" css-class="product-card">
          <mj-image src="https://images.unsplash.com/photo-1594938298603-c8148c4dae35?w=320&h=400&fit=crop" alt="Premium Denim Jeans" border-radius="8px" padding="0" />
          <mj-text font-size="15px" font-weight="600" color="#0a0a0a" align="center" padding="18px 0 6px">
            Premium Denim Jeans
          </mj-text>
          <mj-text font-size="14px" color="#666666" align="center" padding="0 0 8px">
            $129.00
          </mj-text>
          <mj-text align="center" padding="0 0 15px" font-size="11px">
            <span style="color: #F4B942;">★★★★★</span>
            <span style="color: #999; font-size: 11px;"> (56)</span>
          </mj-text>
