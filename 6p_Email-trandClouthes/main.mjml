        <mj-column width="48%" padding="0 10px 0 0" vertical-align="top">
          <mj-image src="https://images.unsplash.com/photo-1523359346063-d879354c0ea5?w=420&h=500&fit=crop&crop=faces" alt="Lifestyle - Couple Walking" border-radius="8px" padding="0" />
