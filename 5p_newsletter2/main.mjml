<mjml>
  <mj-head>
    <mj-font name="Playfair" href="https://fonts.googleapis.com/css2?family=Playfair+Display:ital,wght@0,400..900;1,400..900&display=swap" />
    <mj-font name="Cormorant Garamond" href="https://fonts.googleapis.com/css2?family=Cormorant+Garamond:wght@400;500&display=swap" />
    <mj-attributes>
      <mj-all font-family="Playfair" />
      <mj-text font-size="16px" color="#555555" />
    </mj-attributes>
    <mj-style>
      .nav-container div {
        white-space: nowrap !important;
      }

      .nav-links {
        color: #181818;
        text-decoration: none;
        padding: 0 50px;
        display: inline-block;
      }

      .section-number {
        font-family: 'Cormorant Garamond', Georgia, serif;
        font-size: 13px;
        letter-spacing: 3px;
        color: #111111;
      }

      .section-title {
        font-family: 'Playfair Display', Georgia, serif;
        text-transform: uppercase;
        letter-spacing: 6px;
      }

      .section-description {
        font-family: 'Cormorant Garamond', Georgia, serif;
        line-height: 1.7;
      }

      .divider-line {
        border-top: 1px solid #111111 !important;
        opacity: 0.3;
      }

      @media only screen and (max-width: 480px) {
        .title div {
          font-size: 40px !important;
          line-height: 1.1 !important;
        }

        .description div {
          font-size: 12px !important;
        }

        .hero-section {
          padding: 0 !important;
        }

        .nav-links {
          padding: 0 10px !important;
          font-size: 16px !important;
        }

        .section-title div {
          font-size: 24px !important;
          letter-spacing: 4px !important;
        }

        .section-description div {
          font-size: 13px !important;
          padding-left: 20px !important;
          padding-right: 20px !important;
        }

        .section-number {
          font-size: 12px !important;
        }
        .nav-link {
          padding: 0 12px !important;
          font-size: 11px !important;
        }
      }
    </mj-style>
    <mj-preview>Preview text goes here</mj-preview>
    <mj-title>Email Title</mj-title>
  </mj-head>
  <mj-body background-color="#f4f4f4">
