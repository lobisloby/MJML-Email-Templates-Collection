        <mj-divider border-color="#CFBFAC" border-style="solid" border-width="1px" width="100%" padding="10px 25px" />
      </mj-column>
    </mj-section>
    <!-- SECTION: hero text  -->
    <mj-section background-color="#EBDDCC" padding="0 20% 0 0" css-class="hero-section">
      <mj-column>
