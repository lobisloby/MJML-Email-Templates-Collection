        <mj-button padding-top="30px" inner-padding="8px 40px" background-color="#98C2A6" border="1px solid #181818" color="#181818" font-weight="700" href="yoursite">
          LEARN MORE
        </mj-button>
      </mj-column>
    </mj-section>
    <!-- ========== FOOTER SECTION ========== -->
